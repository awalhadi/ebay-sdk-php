{
  "name": "ah/ebay-sdk-php",
  "description": "An eBay SDK for PHP. Use the eBay API in your PHP projects.",
  "keywords": [
    "ebay",
    "api",
    "sdk"
  ],
  "homepage": "https://portfolio.awalhadi.vercel.app/",
  "type": "library",
  "license": "(Apache-2.0 and MIT)",
  "authors": [
    {
      "name": "Md. A Awal hadi",
      "email": "awalhadi5@gmail.com",
      "homepage": "https://portfolio.awalhadi.vercel.app"
    }
  ],
  "support": {
    "issues": "https://github.com/awalhadi/ebay-sdk-php/issues"
  },
  "require": {
    "php": "^8.1",
    "guzzlehttp/guzzle": "^7.2"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "squizlabs/php_codesniffer": "2.*"
  },
  "autoload": {
    "psr-4": {
      "DTS\\eBaySDK\\": "src/"
    },
    "files": [
      "src/functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "DTS\\eBaySDK\\Test\\": "test/"
    },
    "classmap": [
      "build/"
    ]
  }
}
